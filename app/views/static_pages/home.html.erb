<% if logged_in? %>
  <div class="row">
    <div class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="post_form">
        <%= render 'shared/post_form' %>
      </section>
    </div>
  </div>
  <%= render 'shared/feed' %>
<% else %>

<%= render 'swiper' %>

<% end %>